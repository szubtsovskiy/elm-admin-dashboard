    "babel-loader": "^8.0.0",
    "compass-mixins": "^0.12.10",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.1",
    "elm": "^0.19.0-no-deps",
    "elm-format": "^0.8.1",
    "elm-hot-webpack-loader": "^1.0.2",
